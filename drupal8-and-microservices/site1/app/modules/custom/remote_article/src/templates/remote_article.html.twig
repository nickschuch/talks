{#
/**
 * @file remote_article.html.twig
 * Default theme implementation to present Remote Article data.
 *
 * This template is used when viewing Remote Article pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_remote_article()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('remote_article') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
